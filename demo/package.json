{
  "name": "react-interactive-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@modulz/radix-icons": "^4.0.0",
    "@stitches/react": "^0.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-interactive": "^1.0.0-next.2",
    "react-router-dom": "^5.2.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "browserslist-config-css-grid": "^1.0.0",
    "gh-pages": "^3.1.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "dev": "npm install --save ../ && npm start",
    "devCleanup": "npm install --save react-interactive@next",
    "deploy": "gh-pages --dist build --message Built-`date +%Y%m%d`-`date +%H%M%S`",
    "predeploy": "npm run devCleanup && npm run lint && npm test -- --watchAll=false && npm run build",
    "lint": "eslint src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 2 versions or > 0.2% and not dead and extends browserslist-config-css-grid"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
